{
  "name": "@aureooms/js-algorithms",
  "description": "Playground for algorithms in JavaScript",
  "homepage": "https://aureooms.github.io/js-algorithms",
  "main": "lib/index.js",
  "version": "3.0.7",
  "repository": {
    "url": "https://github.com/aureooms/js-algorithms.git",
    "type": "git"
  },
  "keywords": [
    "adt",
    "algorithm",
    "complexity",
    "ender",
    "javascript",
    "js",
    "template"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "test": "ava",
    "esdoc": "esdoc",
    "lint": "true",
    "prepare": "npm run build",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "travis": "npm run lint && npm run cover",
    "release": "np"
  },
  "author": "aureooms",
  "devDependencies": {
    "@aureooms/js-array": "^3.1.0",
    "@aureooms/js-compare": "^1.4.3",
    "@aureooms/js-functools": "^2.0.3",
    "@aureooms/js-itertools": "^3.0.0",
    "@aureooms/js-operator": "^1.0.2",
    "@aureooms/js-partition": "^8.0.0",
    "@aureooms/js-random": "^1.0.1",
    "@aureooms/js-selection": "^9.0.0",
    "@aureooms/js-sort": "^7.0.0",
    "@aureooms/js-splitting": "^4.0.2",
    "aureooms-node-package": "^6.0.1",
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "ava": "3.13.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.5.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7"
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-algorithms/issues"
  },
  "dependencies": {},
  "license": "AGPL-3.0",
  "spm": {
    "main": "js/dist/algorithms.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "files": [
    "lib"
  ],
  "sideEffects": false
}
